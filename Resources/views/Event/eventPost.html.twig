{% extends 'OutlandishAcadOowpBundle:Default:defaultPost.html.twig' %}

{% block postDate %}<p class="date font-large">{{ post.dateString() }}</p>{% endblock %}

{% block sideBar %}
    {% include "OutlandishAcadOowpBundle:Partial:share.html.twig" with {
    'post': post,
    'title': 'event'} %}

    {% include "OutlandishAcadOowpBundle:Partial:event.html.twig" with {
    'post': post,
    'title': 'event',
    'latitude' : event_latitude,
    'longitude' : event_longitude
    } %}

{% endblock %}

